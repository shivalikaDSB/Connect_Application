<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
 
    <!-- view background color -->
     <solid
        android:color="#FFF0D6" >
         
    </solid>
    <!-- view border color and width -->
    <stroke
        android:width="3dp"
        android:color="#FFF0D6" >
    </stroke>
 
    <!-- If you want to add some padding -->
    <padding
        android:left="10dp"
        android:top="7dp"
        android:right="4dp"
        android:bottom="7dp"    >
    </padding>
 
    <!-- Here is the corner radius -->
    <corners
        android:radius="57dp"   >
    </corners>
 
</shape>
